					"code" : "calcEnv(x, in_high, out_low, out_high, power) {\r\n\tif (out_low < out_high) {\r\n\t\tif (power > 0) {\r\n\t\t\treturn out_low + (out_high - out_low) * (1 - pow(1 - x / in_high, 1 + 2*E*power));\r\n\t\t} else {\r\n\t\t\treturn out_low + (out_high - out_low) * pow(x / in_high, 1 - 2*E*power);\r\n\t\t}\r\n\t} else {\r\n\t\tif (power > 0) {\r\n\t\t\treturn out_high + (out_low - out_high) * pow(1 - x / in_high, 1 + 2*E*power);\r\n\t\t} else {\r\n\t\t\treturn out_high + (out_low - out_high) * (1 - pow(x / in_high, 1 - 2*E*power));\r\n\t\t}\r\n\t}\r\n}\r\n\r\nParam loopmode(min=0, max=2, default=0);  // same as live.adsr~\r\nParam legato(min=0, max=1, default=0);    // same as adsr~\r\nParam retrigger(min=0, default=5);        // same as adsr~\r\n\r\nHistory state;  // 0=idle, 1=retrigger, 2=attack, 3=decay, 4=sustain, 5=release, default is 0\r\nHistory time;   // current time in state (msec)\r\nHistory from;   // to remind initial amplitude of current state\r\nHistory env;    // to output and remind velocity \r\nHistory vel;    // to remind note on velocity in release state\r\n\r\n// check state\r\nif (state != 0 && state != 5 && in1 == 0 ||          // note on -> release\r\n    ((state == 3 || state == 4) && loopmode == 1)) {    \r\n\tstate, time, from = 5, 0, env;\r\n}\r\nif (state == 5 && time >= in6) {                     // end of release\r\n\tif (loopmode == 2) {\t// loop to attack\r\n\t\tstate, time, from = 2, 0, in7;\r\n\t} else {\t\t\t\t// idle\r\n\t\tstate, time = 0, 0;\r\n\t}\r\n}\r\nif (in2 > 0 && state != 0  && state != 1 ||         // envelope on -> note on\r\n    (in1 > 0 && state == 5)) {\r\n\tif (legato == 0) {\t// retrigger\r\n\t\tstate, time, from = 1, 0, env;\r\n\t} else {\t\t\t// attack in legato\r\n\t\tstate, time, from = 2, 0, env;\r\n\t}\r\n} else if (in1 > 0 && state == 0) {    \t\t\t\t// idle -> attack\r\n\tstate, time, from = 2, 0, in7;\r\n}\r\nif (state == 1 && time >= retrigger) {               // retrigger -> attack\r\n\tstate, time, from = 2, 0, in7;\r\n}\r\nif (state == 2 && time >= in3) {                     // attack -> decay\r\n\tstate, time = 3, 0;\r\n}\r\nif (state == 3 && time >= in4) {                     // end of decay\r\n\tif (loopmode == 1 || loopmode == 2) {\t// skip sutain to release\r\n\t\tstate, time, from = 5, 0, in5;\r\n\t} else {\t\t\t\t\t\t\t\t// sustain\r\n\t\tstate, time = 4, 0;\r\n\t}\r\n}\r\n\r\n// obtain env (0 to 1)\r\nif (state == 0) {\t\t\t// idle\r\n\tout2, out3 = 0, 0;\r\n\tenv = in9;\r\n} else if (state == 1) {\t// retrigger\r\n\tout2, out3 = 1, 1;\r\n\ttime += sampstoms(1);\r\n\tenv = calcEnv(time, retrigger, from, in7, 0);\r\n} else if (state == 2) {\t// attack\r\n\tout2, out3 = 1, 1;\r\n\ttime += sampstoms(1);\r\n\tenv = calcEnv(time, in3, from, in8, in10);\r\n\tvel = in1;\r\n} else if (state == 3) {\t// decay\r\n\tout2, out3 = 1, 1;\r\n\ttime += sampstoms(1);\r\n\tenv = calcEnv(time, in4, in8, in5, in11);\r\n\tvel = in1;\r\n} else if (state == 4) {\t// sustain\r\n\tout2, out3 = 1, 1;\r\n\tenv = in5;\r\n\tvel = in1;\r\n} else if (state == 5) {    // release\r\n\tout2, out3 = 1, 0;\r\n\ttime += sampstoms(1);\r\n\tenv = calcEnv(time, in6, from, in9, in12);\r\n}\r\n\r\nout1 = vel * env;\r\n",
